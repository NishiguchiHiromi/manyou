<div class="container">
  <div class="row">
    <%= label_tag :title, t('.new_user'), class: 'title' %>
    <hr />
    <%= form_for @user , url: admin_users_path do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
      <%= f.submit 'Register', style: 'font-size: 16px; float: right;' %>
      <% end %>
      <%= button_to 'Back', admin_users_path, method: :get, class: 'btn btn-warning btn-back link-color' %>
    </div>
  </div>
